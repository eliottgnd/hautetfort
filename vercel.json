{
  "version": 2,
  "buildCommand": "",
  "outputDirectory": "dist",
  "routes": [
    { "src": "/[^.]+", "dest": "/", "status": 200 }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Permissions-Policy",
          "value": "geolocation=(), camera=(), microphone=()"
        }
      ]
    }
  ]
} 